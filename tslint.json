{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
            "options": [80]
    },
    "no-console": false,
    "max-classes-per-file": false,
    "no-empty": false,
    "no-shadowed-variable": false,
    "prefer-for-of": false
  },
  "linterOptions": {
    "exclude": ["./src/gatsby-types.d.ts"]
  }
}
